{
  "compilerOptions": {
    "moduleResolution": "node",
    "module": "commonjs",
    "jsx": "react",
    "target": "es5",
    "noImplicitThis": false,
    "noImplicitReturns": true,
    "noEmitOnError": true,
    "allowSyntheticDefaultImports": true,
    "removeComments": false,
    "experimentalDecorators": true,
    "esModuleInterop": true
  }
}
